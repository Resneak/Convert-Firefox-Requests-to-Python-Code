{
  "manifest_version": 2,
  "name": "Convert to Python Requests",
  "version": "1.0",
  "description": "Convert network requests to Python requests code",
  "permissions": ["webRequest", "webRequestBlocking", "activeTab", "clipboardWrite"],
  "background": {
    "scripts": ["background.js"]
  },
  "devtools_page": "devtools.html",
  "icons": {
    "48": "image1.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "convert_to_python_requests@example.com",
      "strict_min_version": "42.0"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}